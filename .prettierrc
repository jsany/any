{
  "trailingComma": "all",
  "semi": false,
  "singleQuote": true,
  "endOfLine": "lf",
  "printWidth": 120,
  "tabWidth": 2,
  "overrides": [
    {
      "files": ["*.ts","*.tsx"],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": ["*.md"],
      "options": {
        "parser": "markdown"
      }
    },
    {
      "files": [ "*.json"],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [ "*.js"],
      "options": {
        "parser": "babel"
      }
    }
  ]
}
